<html>
<head>
    <title>Codenvy Admin</title>
    <meta name="decorator" content="atl.admin">
    $webResourceManager.requireResource("com.example.tutorial.plugins.new-listener-plugin:resources")
</head>
<body>
<form id="admin" class="aui">
    <div class="field-group">
        <label for="instanceUrl">Instance URL:</label>
        <input type="text" id="instanceUrl" name="instanceUrl" class="text">
    </div>
    <div class="field-group">
        <label for="username">Username:</label>
        <input type="text" id="username" name="username" class="text">
    </div>
    <div class="field-group">
        <label for="password">Password:</label>
        <input type="text" id="password" name="password" class="text">
    </div>
    <div class="field-group">
        <input type="submit" value="Save" class="button">
    </div>
</form>
</body>
</html>